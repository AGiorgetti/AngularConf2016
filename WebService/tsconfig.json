{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./wwwroot/tsc-out",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./node_modules/moment/moment.d.ts",
    "./TypeScript/**/*"    
  ],
  "exclude": [
    "node_modules",
    ".vscode"
  ]
}